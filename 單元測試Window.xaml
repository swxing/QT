<Window x:Class="QT.單元測試Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QT"
        mc:Ignorable="d"
        Title="單元測試Window" d:DesignHeight="355" d:DesignWidth="720" >
    <Grid>
        <DataGrid x:Name="dg" Margin="5,140,10,10" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <WrapPanel Margin="5,15,5,0" Background="#FFFFC6C6" Height="120" VerticalAlignment="Top">
            <Button x:Name="btnCnYesGetBars" Content="GetCnYesBars" Click="btnCnYesGetBars_Click"/>
            <Button x:Name="btnGetSecurity" Content="GetSecuritys" Click="btnGetSecurity_Click"/>
            <Button x:Name="btnUpdateSecuritysToDB" Content="UpdateSecuritysToDB" Click="btnUpdateSecuritysToDB_Click"/>
        </WrapPanel>

    </Grid>
</Window>
